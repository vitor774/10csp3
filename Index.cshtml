@{
    ViewData["Title"] = "SENAI";
}

<div class="container">

    <h1> >>> Sistema de Controle Interno - LH Pet <<< </h1>

            <table class="table table-hover table-sm table-bordered table-sm caption-top">
                <caption>Lista de histórico</caption>
                <thead class="table-dark">
                    <tr>
                        <th scope=" col">Data</th>
                        <th scope="col">Versão</th>
                        <th scope="col">Descrição</th>
                        <th scope="col">Autor</th>
                        
                    </tr>
                </thead>
                <tbody>
                    @foreach (var item in @ViewBag.listahistórico)
                    {

                    <tr>
                        <th scope="row">@item.Data</th>
                        <td>@item.Versão</td>
                        <td>@item.Descrição</td>
                        <td>item.Autor</td>
                        
                    </tr>
                    }
                </tbody>
            </table>
